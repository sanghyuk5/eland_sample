<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="home_section_title">
        <item name="android:textSize">20.16dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#2f2f2f</item>
        <item name="android:layout_marginStart">14.4dp</item>
    </style>

    <style name="home_section_subtitle">
        <item name="android:layout_marginLeft">9.6dp</item>
        <item name="android:textSize">13.44dp</item>
        <item name="android:textColor">#8e8e8e</item>
    </style>

    <style name="DialogBody" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowMinWidthMinor">@fraction/dialog_width</item>
        <item name="android:windowBackground">@drawable/shape_dialog_basic</item>
    </style>

    <style name="DialogCancel">
        <item name="android:background">#787878</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="DialogOk">
        <item name="android:background">#131922</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
    </style>
</resources>